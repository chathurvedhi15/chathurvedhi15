 import datetime
 print("Enter birthday in yyyy:mm:dd format: ")
 bd = datetime.date(int(input()), int(input()), int(input()))
 tdy = datetime.date.today()
 days = (tdy-bd).days
 seconds = days*86400
 print("days: ", days, "seconds: ", seconds)





a = int(input())
 b = int(input())
 c = int(input())
 s = (a+b+c)/3
 print((s*(s-a)*(s-b)*(s-c))**0.5)s
